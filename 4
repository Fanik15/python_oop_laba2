class Employee:
    name=""
    salary=""
myEmployee1 = Employee()
myEmployee2 = Employee()
myEmployee1.age = "22"
myEmployee2.age = "43"
myEmployee1.salare = 2234
myEmployee2.salare = 3000
myEmployee1.name = "Stepan"
myEmployee2.name = "Ivanka"
print(myEmployee1.name, myEmployee2.name)
print(myEmployee1.salare + myEmployee2.salare)
