class Employee:
    name = "Ivanka"
    age = "24"
    salary = "12345$"
myEmployee = Employee()
print(Employee.name + ', ' + Employee.age + ', ' + Employee.salary)
5md
class Employee:
    def show(self):
        return '+++'
myEmployee = Employee()
print(myEmployee.show())
6md
class Employee:
    name="Inanka"
    salary="300$"

    def show(self, name, salary):
        return name + ' ' + salary
myEmployee = Employee()
print(myEmployee.show('Ivanka', '300$'))
